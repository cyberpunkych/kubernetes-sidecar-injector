{
  "name": "kubernetes-sidecar-injector",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "start": "node -r module-alias/register ./dist",
    "dev": "dotenv -e .env nodemon",
    "dev:inspect": "dotenv -e .env nodemon --inspect",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyberpunkyc/kubernetes-sidecar-injector.git"
  },
  "keywords": [
    "sidecar",
    "sidecar-injector",
    "admission-controller",
    "mutating-admission-webhook"
  ],
  "author": "Kareem Khaled",
  "_moduleAliases": {
    "@root": "dist",
    "@lib": "dist/lib",
    "@services": "dist/services",
    "@shared": "dist/shared",
    "@routes": "dist/routes",
    "@type": "dist/types"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.17.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "fast-json-patch": "^3.1.1",
    "helmet": "^5.1.1",
    "http-status-codes": "^2.2.0",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.2",
    "winston": "^3.8.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonfile": "^6.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.22.0",
    "find": "^0.3.0",
    "fs-extra": "^10.1.0",
    "jasmine": "^4.3.0",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
